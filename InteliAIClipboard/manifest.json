{
    "name": "InteliAI ClipBoard",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Captures copied data and sends it to a Nuxt3 API",
    "host_permissions": [
        "http://localhost:3000/*"
    ],
    "permissions": [
      "activeTab",
      "contextMenus",
      "storage"
      
    ],
    "icons": {
        "16": "AI.png",
        "32": "AI.png"

      },
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    }
  }
  